const createTree = (value)=>{
    return {
        data: value,
        children: null
    };
};
const addChild = (node, value)=>{
    const newNode = {
        data: value,
        children: null
    };
    node.children = node.children || []; //node.children 有可能是一个空数组
    node.children.push(newNode);
    return newNode;
};
const tree = createTree(10);
addChild(tree, 20);
console.log(tree);
