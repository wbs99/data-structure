const createTree = (value)=>{
    return {
        data: value,
        children: null
    };
};
const addChild = (node, value)=>{
    const newNode = {
        data: value,
        children: null
    };
    node.children = node.child || [];
    node.children.push(newNode);
    return newNode;
};
const tree = createTree(10);
console.log(tree);
